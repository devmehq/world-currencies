{
    "name": "devmehq/world-currencies",
    "description": "A comprehensive library providing currency information for all world currencies",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "currency",
        "currencies",
        "world-currencies",
        "iso-4217",
        "money",
        "finance",
        "exchange",
        "devme"
    ],
    "homepage": "https://github.com/devmehq/world-currencies",
    "authors": [
        {
            "name": "DevMe",
            "email": "support@dev.me",
            "homepage": "https://dev.me"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "DevMe\\WorldCurrencies\\": "src/php/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevMe\\WorldCurrencies\\Tests\\": "tests/php/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs": "phpcs",
        "cs-fix": "phpcbf"
    },
    "support": {
        "issues": "https://github.com/devmehq/world-currencies/issues",
        "source": "https://github.com/devmehq/world-currencies"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/devmehq"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}